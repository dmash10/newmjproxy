# Railway Environment Variables Configuration
# Copy this file to .env for local development or set these in Railway dashboard

# Discord Configuration (REQUIRED)
DISCORD_GUILD_ID=your_discord_server_id_here
DISCORD_CHANNEL_ID=your_discord_channel_id_here
DISCORD_USER_TOKEN=your_discord_user_token_here

# API Security (REQUIRED)
MJ_API_SECRET=your_secure_api_secret_here

# Optional Configuration
MJ_CORE_SIZE=3
MJ_QUEUE_SIZE=10
MJ_TIMEOUT_MINUTES=5
MJ_TASK_STORE_TYPE=in_memory
MJ_TASK_STORE_TIMEOUT=30d
MJ_TRANSLATE_WAY=null
MJ_NOTIFY_POOL_SIZE=10

# Spring Profile (Railway will automatically set this)
SPRING_PROFILES_ACTIVE=railway

# Railway automatically sets PORT, but you can override for local testing
# PORT=8080 